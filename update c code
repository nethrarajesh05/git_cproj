// C program to add two numbers
#include <stdio.h>

int main() {
    int a, b, c, sum = 0;
  
  	// Read two numbers from the user
    printf("Enter two integers: ");
    scanf("%d %d", &a, &b , &c);

    // Calculate the addition of a and b 
//this is intergater addition
    // using '+' operator
    sum = a + b;

    printf("Sum: %d", sum);

    return 0;
}
